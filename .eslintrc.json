{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "new-cap": ["off"],
        "comma-dangle": ["warn", "always-multiline"],
        "no-console": ["off"],
        "no-unused-vars": ["warn"]
    }
}

